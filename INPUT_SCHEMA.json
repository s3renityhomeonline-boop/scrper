{
  "title": "CarGurus Stealth Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "Start URLs",
      "type": "array",
      "description": "List of CarGurus URLs to scrape",
      "default": [
        {
          "url": "https://www.cargurus.ca/Cars/l-Used-SUV-Crossover-bg7"
        }
      ],
      "editor": "requestListSources"
    },
    "maxConcurrency": {
      "title": "Max Concurrency",
      "type": "integer",
      "description": "Number of parallel browsers (keep at 1 for stealth)",
      "default": 1,
      "minimum": 1,
      "maximum": 5
    },
    "maxResults": {
      "title": "Max Results",
      "type": "integer",
      "description": "Maximum number of cars to scrape",
      "default": 10,
      "minimum": 1
    },
    "proxyConfiguration": {
      "title": "Proxy Configuration",
      "type": "object",
      "description": "Proxy settings (required for stealth)",
      "editor": "proxy",
      "default": {
        "useApifyProxy": true,
        "apifyProxyGroups": ["RESIDENTIAL"],
        "apifyProxyCountry": "CA"
      }
    }
  },
  "required": ["startUrls"]
}
